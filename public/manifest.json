{
  "short_name": "Readol",
  "name": "Shareable Reading Lists",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "http://*/*", 
    "https://*/*"
  ],
  "browser_action": {
    "default_popup": "../index.html",
    "default_icon": {
      "16": "logo.jpg",
      "32": "logo.jpg",
      "48": "logo.jpg",
      "128": "logo.jpg" 
    }
  },
  "icons": {
    "16": "logo.jpg",
    "32": "logo.jpg",
    "48": "logo.jpg",
    "128": "logo.jpg"
  },
  "content_security_policy": "script-src 'self' 'sha256-fdAfMKZtCTRPKMRznGwonJHC++vCCrQS7XFnVrzSXAE=' 'sha256-YD8w9VLrdvqI5/tFVmpT+1337ly+e/JvPrkRxYZo/w4='; object-src 'self';"
}